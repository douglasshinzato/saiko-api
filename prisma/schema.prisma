generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id          String   @id @default(uuid())
  barcode     String   @unique
  brand       String
  name        String
  category    String
  description String?
  price       Decimal
  stock       Float?
  createAt    DateTime @default(now())
  updateAt    DateTime @updatedAt
}
